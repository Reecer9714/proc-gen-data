{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "params",
  "description": "Schema for parameter definition",
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^([a-zA-Z0-9_]+)$": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "type": "integer"
        }
      ]
    }
  },
  "examples": ["$self"]
}
